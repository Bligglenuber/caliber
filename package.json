{
  "name": "caliber",
  "version": "0.1.0-snapshot.0",
  "preferGlobal": true,
  "analyze": false,
  "description": "Really simple package manager for your own private packages hosted on any git or svn server. No registry or server hosting required. Can be used in conjunction with bower/npm",
  "main": "index.js",
  "bin": {
    "caliber": "./bin/caliber"
  },
  "scripts": {
    "start": "caliber",
    "test": "mocha --require babel-register --require babel-polyfill",
    "lint": "eslint js test testutil",
    "lintfix": "eslint js test testutil --fix"
  },
  "author": "Pete Ward <peteward44@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "fs-extra": "^2.0.0",
    "git-svn-interface": "^0.1.8",
    "lodash": "^4.17.4",
    "semver": "^5.3.0",
    "uuid": "^3.0.1",
    "yargs": "^6.6.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.23.0",
    "mocha": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peteward44/caliber.git"
  },
  "keywords": [
    "cli",
    "bower",
    "install",
    "modules",
    "package manager"
  ],
  "files": [
    "lib",
    "index.js",
    "bin",
    "npm-shrinkwrap.json",
    "README.md"
  ]
}
